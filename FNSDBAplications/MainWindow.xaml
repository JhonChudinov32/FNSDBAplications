        <Label Content="Обновление по типу 1:" HorizontalAlignment="Left" Margin="70,102,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.596,-0.871" Width="278"/>
        <Label Content="Обновление по типу 3:" HorizontalAlignment="Left" Margin="70,258,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.596,-0.871" Width="278"/>
